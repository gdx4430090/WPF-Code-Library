<Window x:Class="WpfPictureProcessing.EmguImageHandle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPictureProcessing"
        mc:Ignorable="d"
        Title="EmguImageHandle" Height="650" Width="800">
    <StackPanel>
        <Image x:Name="img1" Height="200" Visibility="Collapsed"/>

        <Image x:Name="img2" Height="200" Visibility="Collapsed"/>

        <Image x:Name="img3" Height="Auto"/>

        <Image x:Name="img4" />

    </StackPanel>
</Window>
